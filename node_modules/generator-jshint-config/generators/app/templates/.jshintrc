{
  "esversion":     <%= esversion %>,
  <% if (enforcing) { %>
  "bitwise":       <%= bitwise %>,
  "curly":         <%= curly %>,
  "eqeqeq":        <%= eqeqeq %>,
  "forin":         <%= forin %>,
  "freeze":        <%= freeze %>,
  "futurehostile": <%= futurehostile %>,
  "latedef":       <%= latedef %>,
  "noarg":         <%= noarg %>,
  "nocomma":       <%= nocomma %>,
  "nonbsp":        <%= nonbsp %>,
  "nonew":         <%= nonew %>,
  "singleGroups":  <%= singleGroups %>,
  "trailingcomma": <%= trailingcomma %>,
  "undef":         <%= undef %>,
  "varstmt":       <%= varstmt %>,
  "unused":        <%= unused %>,
  "maxcomplexity": <%= maxcomplexity %>,
  "maxdepth":      <%= maxdepth %>,
  "maxerr":        <%= maxerr %>,
  "maxparams":     <%= maxparams %>,
  "maxstatements": <%= maxstatements %>,
  "shadow":        <%= shadow %>,
  "strict":        <%= strict %>,
  <% } %>
  <% if (relaxing) { %>
  "asi":           <%= asi %>,
  "boss":          <%= boss %>,
  "debug":         <%= debug %>,
  "eqnull":        <%= eqnull %>,
  "evil":          <%= evil %>,
  "expr":          <%= expr %>,
  "funcscope":     <%= funcscope %>,
  "iterator":      <%= iterator %>,
  "lastsemic":     <%= lastsemic %>,
  "loopfunc":      <%= loopfunc %>,
  "notypeof":      <%= notypeof %>,
  "noyield":       <%= noyield %>,
  "plusplus":      <%= plusplus %>,
  "proto":         <%= proto %>,
  "scripturl":     <%= scripturl %>,
  "supernew":      <%= supernew %>,
  "validthis":     <%= validthis %>,
  "withstmt":      <%= withstmt %>,
  <% } %>
  "browser":       <%= env.browser ? true : false %>,
  "browserify":    <%= env.browserify ? true : false %>,
  "couch":         <%= env.couch ? true : false %>,
  "devel":         <%= env.devel ? true : false %>,
  "dojo":          <%= env.dojo ? true : false %>,
  "jasmine":       <%= env.jasmine ? true : false %>,
  "jquery":        <%= env.jquery ? true : false %>,
  "mocha":         <%= env.mocha ? true : false %>,
  "mootools":      <%= env.mootools ? true : false %>,
  "node":          <%= env.node ? true : false %>,
  "nonstandard":   <%= env.nonstandard ? true : false %>,
  "phantom":       <%= env.phantom ? true : false %>,
  "prototypejs":   <%= env.prototypejs ? true : false %>,
  "qunit":         <%= env.qunit ? true : false %>,
  "rhino":         <%= env.rhino ? true : false %>,
  "shelljs":       <%= env.shelljs ? true : false %>,
  "typed":         <%= env.typed ? true : false %>,
  "worker":        <%= env.worker ? true : false %>,
  "wsh":           <%= env.wsh ? true : false %>,
  "yui":           <%= env.yui ? true : false %>
}
